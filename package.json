{
    "name": "use-build",
    "version": "0.1.0-rc.1",
    "description": "A vite plugin run script at build time and import at runtime",
    "homepage": "https://github.com/Shad02w/vite-plugin-use-build",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Shad02w/vite-plugin-use-build.git"
    },
    "type": "module",
    "keywords": [
        "vite",
        "plugin",
        "build"
    ],
    "files": [
        "dist",
        "README.md"
    ],
    "exports": {
        "./vite": {
            "import": "./dist/vite.js",
            "require": "./dist/vite.cjs"
        },
        "./rsbuild": {
            "import": "./dist/rsbuild.js",
            "require": "./dist/rsbuild.cjs"
        },
        "./loader": {
            "import": "./dist/rspack/loader.js",
            "require": "./dist/rspack/loader.cjs"
        },
        "./rsbuildv2": {
            "import": "./dist/rsbuildv2/index.js",
            "require": "./dist/rsbuildv2/index.cjs"
        }
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "prepublish": "pnpm run build"
    },
    "dependencies": {
        "@swc/core": "1.7.24",
        "memfs": "4.11.1",
        "picocolors": "1.1.0"
    },
    "devDependencies": {
        "@rsbuild/core": "1.0.1-rc.5",
        "@rspack/core": "1.0.3",
        "@types/express": "4.17.21",
        "@types/node": "22.5.4",
        "prettier": "3.3.3",
        "tsup": "8.2.4",
        "typescript": "5.5.4",
        "vite": "5.4.3"
    },
    "peerDependencies": {
        "@rsbuild/core": ">=1.0.0",
        "@rspack/core": ">=1.0.0",
        "vite": ">=5.1.0"
    },
    "peerDependenciesMeta": {
        "vite": {
            "optional": true
        },
        "@rsbuild/core": {
            "optional": true
        },
        "@rspack/core": {
            "optional": true
        }
    },
    "license": "MIT"
}
