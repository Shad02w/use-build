{
    "name": "vite-plugin-use-build",
    "version": "0.0.5",
    "description": "A vite plugin run script at build time and import at runtime",
    "homepage": "https://github.com/Shad02w/vite-plugin-use-build",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Shad02w/vite-plugin-use-build.git"
    },
    "type": "module",
    "keywords": [
        "vite",
        "plugin",
        "build"
    ],
    "files": [
        "dist",
        "README.md"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup-node ./src/index.ts --dts --format esm,cjs --out-dir ./dist",
        "dev": "tsup-node ./src/index.ts --watch --dts --format esm,cjs --out-dir ./dist",
        "prepublish": "pnpm run build"
    },
    "dependencies": {
        "@swc/core": "^1.4.11"
    },
    "devDependencies": {
        "@types/node": "^20.12.2",
        "prettier": "^3.2.5",
        "tsup": "^8.0.2",
        "typescript": "^5.4.3",
        "vite": "^5.2.7"
    },
    "peerDependencies": {
        "vite": ">=5.1.0"
    },
    "license": "MIT"
}
