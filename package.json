{
    "name": "use-build",
    "version": "0.0.4",
    "description": "A vite plugin run script at build time and import at runtime",
    "homepage": "https://github.com/Shad02w/vite-plugin-use-build",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Shad02w/vite-plugin-use-build.git"
    },
    "type": "module",
    "keywords": [
        "vite",
        "plugin",
        "build"
    ],
    "files": [
        "dist",
        "README.md"
    ],
    "exports": {
        "./vite": {
            "import": "./dist/vite.js",
            "require": "./dist/vite.cjs"
        },
        "./rsbuild": {
            "import": "./dist/rsbuild.js",
            "require": "./dist/rsbuild.cjs"
        },
        "./loader": {
            "import": "./dist/rspack/loader.js",
            "require": "./dist/rspack/loader.cjs"
        }
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "prepublish": "pnpm run build"
    },
    "dependencies": {
        "@swc/core": "^1.4.11",
        "memfs": "^4.9.3",
        "picocolors": "^1.0.1"
    },
    "devDependencies": {
        "@rsbuild/core": "^0.6.15",
        "@rspack/core": "^0.6.5",
        "@types/express": "^4.17.21",
        "@types/node": "^20.12.2",
        "prettier": "^3.2.5",
        "tsup": "^8.0.2",
        "typescript": "^5.4.3",
        "vite": "^5.2.7"
    },
    "peerDependencies": {
        "@rsbuild/core": ">=0.6.4",
        "@rspack/core": ">=0.6.2",
        "vite": ">=5.1.0"
    },
    "peerDependenciesMeta": {
        "vite": {
            "optional": true
        }
    },
    "license": "MIT"
}
